{
  "$schema": "https://assets.empirical.run/config/schema/v0.4.0.json",
  "runs": [
    {
      "type": "model",
      "provider": "google",
      "model": "gemini-1.0-pro",
      "prompt": "{{user_message}}",
      "parameters": {
        "stopSequences": ["Asia"]
      }
    },
    {
      "type": "model",
      "provider": "google",
      "model": "gemini-1.5-pro",
      "prompt": "{{user_message}}",
      "parameters": {
        "stopSequences": ["Asia"]
      }
    }
  ],
  "dataset": {
    "samples": [
      {
        "inputs": {
          "user_message": "What is the largest continent in the world?"
        }
      }
    ]
  }
}
