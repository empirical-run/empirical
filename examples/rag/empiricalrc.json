{
  "$schema": "https://assets.empirical.run/config/schema/latest.json",
  "runs": [
    {
      "type": "py-script",
      "path": "rag.py",
      "parameters": {
        "model": "gpt-3.5-turbo"
      },
      "scorers": [
        {
          "type": "py-script",
          "path": "score.py"
        }
      ]
    },
    {
      "type": "model",
      "provider": "openai",
      "model": "gpt-3.5-turbo",
      "prompt": "{{question}}"
    }
  ],
  "dataset": {
    "path": ".empiricalrun/dataset.jsonl"
  }
}